<?xml version="1.0" encoding="UTF-8"?>
<network>
    <constant name="port">8080</constant>
    <servers>
        <array>
            <string>localhost</string>
            <string>127.0.0.1</string>
            <compute>80 8080 +</compute>
        </array>
    </servers>
    <timeout>
        <compute>1000 500 +</compute>
    </timeout>
</network>